[
  {
    "name": "link-resolver",
    "image": "245954399621.dkr.ecr.eu-west-1.amazonaws.com/link-resolver",
    "cpu": 256,
    "memory": 256,
    "essential": true,
    "portMappings": [
      {
        "hostPort": "${external}",
        "protocol": "${protocol}",
        "containerPort": "${internal}"
      }
    ],"${docker_ports}",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "lg_prod",
        "awslogs-region": "eu-west-1",
        "awslogs-stream-prefix": "ls_linkresolver"
      }
    }
  }
]